{
  "name": "@erento/nestjs-module-google-pubsub",
  "version": "2.2.0",
  "description": "NestJS module to work with Google Pub/Sub messages.",
  "main": "dist/public-api.js",
  "types": "dist/public-api.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --fix -c .eslintrc.json",
    "lint-staged": "lint-staged",
    "prebuild": "rm -rf dist && npm run lint && npm run test",
    "test": "jest"
  },
  "keywords": [],
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/erento/nestjs-modules.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": ">=8.0.0",
    "typescript": ">=4.0.0"
  },
  "dependencies": {
    "@erento/eslint-plugin-erento-rules": "1.0.0",
    "@google-cloud/pubsub": "2.18.5",
    "message-crypto": "1.0.0",
    "object-hash": "2.2.0"
  },
  "devDependencies": {
    "@nestjs/common": "8.3.0",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "eslint": "8.6.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.5.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "40.0.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.4",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "precommit": [
    "lint-staged"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
