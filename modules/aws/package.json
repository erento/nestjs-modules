  "description": "NestJS module for communication with AWS.",
  "main": "dist/public_api.js",
  "types": "dist/public_api.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "tslint -c tslint.json --fix -e \"**/node_modules/**\" -p ./tsconfig.json",
    "lint-staged": "lint-staged",
    "prebuild": "rm -rf dist && npm run lint && npm run test",
    "test": "echo \"Error: no test specified\""
  },
  "keywords": [],
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/erento/nestjs-modules.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": ">=5.3",
    "typescript": ">=2.7"
  },
  "dependencies": {
    "aws-sdk": "2.477.0"
  },
  "devDependencies": {
    "@nestjs/common": "6.3.1",
    "@types/node": "11.13.14",
    "husky": "2.4.1",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.2",
    "tslint": "5.17.0",
    "tslint-eslint-rules": "5.4.0",
    "typescript": "3.4.5"
  },
  "precommit": [
    "lint-staged"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "git add"
    ]
  }
