{
  "name": "@erento/nestjs-module-database",
  "version": "3.1.0",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "description": "NestJS module to work with DB with sequelize.",
  "main": "dist/public-api.js",
  "types": "dist/public-api.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --fix -c .eslintrc.json",
    "lint-staged": "lint-staged",
    "prebuild": "rm -rf dist && npm run lint && npm run test",
    "test": "jest"
  },
  "keywords": [],
  "author": "Erento GmbH <developers@erento.com> (https://www.erento.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/erento/nestjs-modules.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "@nestjs/common": ">=8.0.0",
    "typescript": ">=4.0.0",
    "sequelize": ">=6.0.0",
    "sequelize-typescript": ">=2.0.0"
  },
  "dependencies": {
    "@erento/eslint-plugin-erento-rules": "1.0.0",
    "sequelize": "6.16.1",
    "sequelize-typescript": "2.1.3"
  },
  "devDependencies": {
    "@nestjs/common": "8.3.1",
    "@types/jest": "27.4.0",
    "@types/node": "17.0.18",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "eslint": "8.9.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "37.9.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "40.1.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "lint-staged": "12.3.4",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.5.4",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5"
  },
  "precommit": [
    "lint-staged"
  ],
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
